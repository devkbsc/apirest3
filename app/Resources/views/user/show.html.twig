{% extends 'PetkoparaCrudGeneratorBundle/base.html.twig' %}

{% block title %}
{{ parent() }} - User
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View user <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ user.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Name</strong></p>
        <p>
            {{ user.name }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Username</strong></p>
        <p>
            {{ user.username }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Email</strong></p>
        <p>
            {{ user.email }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Address</strong></p>
        <p>
            {{ user.address }}
        </p>
    </div>

    <div class="col-md-6">
        <p><strong>Phone</strong></p>
        <p>
            {{ user.phone }}
        </p>
    </div>

    <div class="col-md-6">
        <p><strong>Website</strong></p>
        <p>
            {{ user.website }}
        </p>
    </div>

    <div class="col-md-6">
        <p><strong>Company</strong></p>
        <p>
            {{ user.company }}
        </p>
    </div>

    </div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('user') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
</div>


{% endblock %}

